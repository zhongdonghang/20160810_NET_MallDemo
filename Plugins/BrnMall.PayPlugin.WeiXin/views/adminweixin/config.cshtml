@model BrnMall.PayPlugin.WeiXin.models.ConfigModel
           
<script type="text/javascript" src="/admin_mall/scripts/jquery.validate.js"></script>
<script type="text/javascript" src="/admin_mall/scripts/jquery.validate.unobtrusive.js"></script>
<h1 class="rightH1">
插件管理 &gt;&gt; 插件配置
<div class="right">
<div class="clear"></div>
</div>
<div class="clear"></div>
</h1>
@using (Html.BeginForm("config", "adminweixin"))
{
<div class="addTable">
<table width="100%">
 <tr>
    <td align="right">商户支付密钥：</td>
    <td>
    @Html.TextBoxFor(x => x.Key, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.Key)
    </td>
    </tr>
          <tr>
    <td align="right">公众帐号secert：</td>
    <td>
    @Html.TextBoxFor(x => x.AppSecret, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.AppSecret)
    </td>
    </tr>
  <tr style="background-color:#DFDEED">
    <td width="180px" align="right">微商城、扫码支付商户信息：</td>
    </tr>
  <tr>
    <td width="120px" align="right">绑定支付的APPID：</td>
    <td>
    @Html.TextBoxFor(x => x.AppId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.AppId)
    </td>
    </tr>
      <tr>
    <td align="right">商户号：</td>
    <td>
    @Html.TextBoxFor(x => x.MchId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.MchId)
    </td>
    </tr>
   <tr style="background-color:#DFDEED">
    <td width="180px" align="right">IOS支付商户信息：</td>
    </tr>
  <tr>
    <td width="120px" align="right">绑定支付的APPID：</td>
    <td>
    @Html.TextBoxFor(x => x.IosAppId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.IosAppId)
    </td>
    </tr>
      <tr>
    <td align="right">商户号：</td>
    <td>
    @Html.TextBoxFor(x => x.IosMchId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.IosMchId)
    </td>
    </tr>
    <tr style="background-color:#DFDEED">
    <td width="180px" align="right">Android商户号支付商户信息：</td>
    </tr>
  <tr>
    <td width="120px" align="right">绑定支付的APPID：</td>
    <td>
    @Html.TextBoxFor(x => x.AndroidAppId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.AndroidAppId)
    </td>
    </tr>
      <tr>
    <td align="right">商户号：</td>
    <td>
    @Html.TextBoxFor(x => x.AndroidMchId, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.AndroidMchId)
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>
    <a href="" class="addBt" id="sumbitBut" ><img src="/admin_mall/content/images/submit.jpg" /></a>
            <script type="text/javascript">
                $(function () {
                    $("#sumbitBut").click(function () {
                        $("form:first").submit();
                        return false;
                    })
                });
        </script>
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>&nbsp;</td>
    </tr>
</table>
</div>
}